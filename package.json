{
    "name": "git-merger",
    "displayName": "Git Merger",
    "icon": "merger-icon.png",
    "description": "Simplifies the git merge process",
    "version": "0.3.0",
    "publisher": "shaharkazaz",
    "repository": {
        "type": "git",
        "url": "https://github.com/shaharkazaz/vscode-git-merger"
    },
    "license": "MIT",
    "engines": {
        "vscode": "^1.13.0"
    },
    "galleryBanner": {
        "color": "BDE4A7"
    },
    "keywords": [
        "git",
        "merge",
        "merging",
        "stash",
        "unstash",
        "shelf",
        "util"
    ],
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.gitMergeFrom",
        "onCommand:extension.gitAbortMerge",
        "onCommand:extension.gitStash",
        "onCommand:extension.gitUnstash",
        "onCommand:extension.gitClearStash",
        "onCommand:extension.gitDropStash",
        "onCommand:extension.gitPop"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.gitMergeFrom",
                "title": "GitMerger: Merge from"
            },
            {
                "command": "extension.gitAbortMerge",
                "title": "GitMerger: Abort merge"
            },
            {
                "command": "extension.gitStash",
                "title": "GitMerger: Stash"
            },
            {
                "command": "extension.gitUnstash",
                "title": "GitMerger: Unstash"
            },
            {
                "command": "extension.gitClearStash",
                "title": "GitMerger: Clear stash"
            },
            {
                "command": "extension.gitDropStash",
                "title": "GitMerger: Drop stash"
            },
            {
                "command": "extension.gitPop",
                "title": "GitMerger: Pop"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "moment": "^2.18.1"
    }
}
