{
    "name": "git-merger",
    "displayName": "Git Merger",
    "icon": "merger-icon.png",
    "description": "Simplifies the git merge process",
    "version": "0.2.1",
    "publisher": "ShaharKazaz",
    "repository": {
        "type": "git",
        "url": "https://github.com/shaharkazaz/vscode-git-merger"
    },
    "license": "MIT",
    "engines": {
        "vscode": "^1.13.0"
    },
    "keywords":[
        "git",
        "merge",
        "merging"
    ],
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.gitMergeFrom",
        "onCommand:extension.gitAbortMerge"
    ],
    "main": "./out/src/commands/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.gitMergeFrom",
                "title": "Git: Merge From"
            },
            {
                "command": "extension.gitAbortMerge",
                "title": "Git: Abort Merge"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
